@model IEnumerable<Practical13Test02.Models.DesingationCount>
<div>
    <h4>Employee count Designation wise.</h4>
    <hr />
    <table class="table table-dark table-striped table-bordered">
        <tr>
            <th>@Html.DisplayNameFor(model => model.DesignationName)</th>
            <th>EmployeeCount</th>
        </tr>

        @{

            foreach (var item in Model)

            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.DesignationName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Count)
                    </td>
                </tr>
             }
               

            }


        </table>
   
</div>
<p>
    @Html.ActionLink("Back to List", "Index", new { }, new { @class = "btn btn-info mx-2 mt-2" })
</p>

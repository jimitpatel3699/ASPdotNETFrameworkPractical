@model IEnumerable<Practical12.Models.Designation>
@{
    ViewData["Title"] = "Designations Queries";

}

<h3>Designations with more than one employees</h3>
@if (Model != null && Model.Count() > 0)
{
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.DesignationName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EmployeesCount)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td style="vertical-align:middle">
                        @Html.DisplayFor(modelItem => item.DesignationName)
                    </td>
                    <td style="vertical-align:middle">
                        @Html.DisplayFor(modelItem => item.EmployeesCount)
                    </td>
                </tr>
            }
        </tbody>
    </table>
}

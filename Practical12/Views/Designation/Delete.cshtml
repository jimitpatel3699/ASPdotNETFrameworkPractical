@model Practical12.Models.Designation
@{
    ViewData["Title"] = "Delete a designation";
   
}
<div class="container-fluid">
    <h5>Are you sure you want to delete this?</h5>

    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DesignationName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DesignationName)
        </dd>
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <input hidden value="@Model.Id" />
        <input type="submit" value="Delete" class="btn btn-danger mb-3" />
    }
    <div>
        @Html.ActionLink("Back to List", "Index")
    </div>
</div>
